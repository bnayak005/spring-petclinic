pipeline {
  agent any
  stages {
    stage('Initialization') {
      steps {
        echo 'echo \'Execution started!!!\''
      }
    }

    stage('checkout') {
      steps {
        echo 'echo \'checking out the code!!!\''
      }
    }

    stage('build') {
      steps {
        echo 'echo \'building out the code!!!\''
      }
    }

    stage('publish') {
      steps {
        echo 'echo \'publishing the result!!!\''
      }
    }

  }
}