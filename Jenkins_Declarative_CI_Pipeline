pipeline{
agent any
stages{
stage('checkout'){
steps{
    echo 'checking out!!!'
	}
}
stage('build'){
steps{
    echo 'building project!!!'
	}
}
stage('archive'){
steps{
    echo 'archiving jar!!!'
	}
}
stage('publish junit result'){
steps{
    echo 'publishing result!!!'
	}
}
stage('notify'){
steps{
    echo 'sending email!!!'
	}
}
}
}